{
  "name": "expressjs-typescript-mongodb-starter",
  "version": "1.0.0",
  "description": "A starter template for building RESTful APIs with Express.js, TypeScript, and MongoDB.",
  "keywords": [
    "express",
    "typescript",
    "mongodb",
    "starter"
  ],
  "author": "Rabin Shrestha",
  "repository": {
    "type": "git",
    "url": "https://github.com/rawbinn/Express.js-TypeScript-MongoDB-Starter"
  },
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "test": "jest",
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "license": "MIT"
}
